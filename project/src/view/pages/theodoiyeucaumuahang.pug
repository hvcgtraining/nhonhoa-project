extends ../layouts/layout.pug
block main-content
    .page-container
        include ../patials/sidebar.pug
        .page-content
            h1.text-center Theo dõi yêu cầu mua hàng
            .panel.panel-default(style='margin-bottom:15px;')
                .panel-heading
                    h6.panel-title
                        i.fas.fa-search(style='line-height:1.5')
                        |  Theo dõi
                .panel-body
                    .row
                        .col-sm-6
                            .form-group.col-sm-6.mg-t1.mg-b1
                                label Số phiếu yêu cầu:
                                input.form-control(type='text',placeholder="Bộ")
                            .form-group.col-sm-6.mg-t1.mg-b1
                                label.control-label Ngày Lập: 
                                input.form-control(type='date', name="date")
                            .form-group.col-sm-12.mg-t1.mg-b1
                                label Y/c KT:
                                input.form-control(type='text')
                            .form-group.col-sm-12.mg-t1.mg-b1
                                label V/v:
                                input.form-control(type='text')
                        .col-sm-6
                            .form-group.col-sm-6.mg-t1.mg-b1
