                                input.form-control(type='date', name="date")
                            .form-group.col-sm-6.mg-t1.mg-b1
                                label.control-label Ngày hoàn tất: 
                                input.form-control(type='date', name="date")
                            .form-group.col-sm-12.mg-t1.mg-b1
                                label Địa điểm:
                                input.form-control(type='text',placeholder="Thủ Đức, TPHCM")
                            .form-group.col-sm-12.mg-t1.mg-b1
                                label Mục đích:
                                input.form-control(type='text',placeholder="phục vụ sản xuất")
